{
  "elixir.pathToElixirLS": "elixir-ls",
  "Lua.diagnostics.globals": ["vim", "use"],
  "snippets.ultisnips.pythonPrompt": false,
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    },
    "ruby-lsp": {
      "command": "ruby-lsp",
      "initializationOptions": {
        "experimentalFeaturesEnabled": true,
        "enabledFeatures": [
          "codeActions",
          "diagnostics",
          "documentHighlights",
          "documentLink",
          "documentSymbols",
          "foldingRanges",
          "formatting",
          "hover",
          "inlayHint",
          "onTypeFormatting",
          "selectionRanges",
          "semanticHighlighting",
          "completion"
        ],
        "formatter": "auto"
      },
      "filetypes": ["ruby"]
    }
  },
  "coc.preferences.extensionUpdateCheck": "daily"
}
